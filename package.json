{
  "name": "bet",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.build.json && tsc-alias",
    "start": "node -r tsconfig-paths/register dist/server.js",
    "startTS": "ts-node -r tsconfig-paths/register src/server.ts",
    "prisma:dev": "npx prisma generate && npx prisma migrate dev",
    "prisma:deploy": "npx prisma generate && npx prisma migrate deploy",
    "dev": "cross-env NODE_ENV=development nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register ./src/server.ts' -e ts",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests --runInBand"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.5.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8"
  },
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "express-async-errors": "^3.1.1",
    "http-status": "^1.7.3",
    "joi": "^17.11.0",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^4.2.0"
  }
}
